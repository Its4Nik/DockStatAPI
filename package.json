{
  "name": "dockstatapi",
  "version": "2",
  "description": "API for docker hosts using dockerode",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:trace": "nodemon --trace-warnings server.js",
    "offline": "OFFLINE=true nodemon server.js",
    "dep": "bash ./utils/createDependencyGraph.sh",
    "dep:remove": "bash ./utils/removeUnusedDeps.sh && bash ./utils/createDependencyGraph.sh"
  },
  "keywords": [],
  "author": "Its4Nik",
  "license": "BSD 3-Clause License",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dockerode": "^4.0.2",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "dependency-cruiser": "^16.5.0",
    "nodemon": "^3.1.7"
  },
  "nodemonConfig": {
    "ignore": [
      "**/logs/**",
      "**/data/**"
    ],
    "delay": 2500
  }
}
